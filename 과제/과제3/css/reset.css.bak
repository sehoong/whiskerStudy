*{margin:0; padding:0;}
html,body{margin:0; padding:0;}
a{text-decoration:none; color:#000;}
ul,ol,li{list-style:none;}
div,p,span,ul,li,ol,h1,h2,h3,h4,h5,h6{margin:0;padding:0;}


